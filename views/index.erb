<header>
  <h1>Monday Morning Music</h1>
  <nav>
    <a href="/songs/new">
      <button class="nav_Btn">Add</button>
    </a>
    <a href="/playlist">
      <button class="nav_Btn">Playlist</button>
    </a>
    <a href="/login">
      <button class="nav_Btn">Login</button>
    </a>
    <a href="/register">
      <button class="nav_Btn">Register</button>
    </a>
  </nav>
</header>




<section class="artwork_container">
  <% @songs.each do |song| %>
    <a class="song_homepage" href="/songs/<%= song.id %>">
      <img class="img_homepage" src="<%= song.artwork_url%>" alt="">
      <p><%= song.title%></p>
    </a>
  <% end %>
</section>