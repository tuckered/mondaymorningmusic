<header>
  <h1>My Monday Music</h1>
  <nav>
  <a href="/">
    <button class="nav_Btn">Home</button>
  </a>
  <a href="/songs/new">
    <button class="nav_Btn">Add</button>
  </a>
  <% if !logged_in? %>
    <a href="/login">
      <button class="nav_Btn">Login</button>
    </a>
    <a href="/register">
      <button class="nav_Btn">Register</button>
    </a>
  <% end %>
  </nav>
</header>


<iframe width="560" height="315" src="<%= @song['song_url'].sub('watch?v=', 'embed/') %>" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>


<% @user.songs.each do |track| %>

  <p>
    
    <p><%= track.title %> </p>
    <p><%= track.artist %> </p>
    <p><img class="playlist_img" src="<%= track.artwork_url %>" alt=""> </p>

  </p>

<% end %>





<a href="/">
  <button>Home</button>
</a>